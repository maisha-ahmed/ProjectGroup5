<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="United Health Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <!--<parameter name="os_version" value="Yosemite"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="79.0"/>
    <parameter name="url" value="https://www.uhc.com"/>

    <test name = "UnitedHealthCareHome">
        <classes>
            <class name="home.AdditionalInsurancePlans"/>
            <class name="home.CareerTest"/>
            <class name="home.ProductsTest"/>
            <class name="home.SearchButtonTest"/>
            <class name="home.UnitedHealthCareTest"/>
            <class name="home.HealthyLiving"/>



        </classes>
    </test>

</suite>